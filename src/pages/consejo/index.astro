---
import PageLayout from "@layouts/PageLayout.astro"
import TopLayout from "@layouts/TopLayout.astro"
import BottomLayout from "@layouts/BottomLayout.astro"
import { WORK } from "@consts"

---

<PageLayout title={WORK.TITLE} description={WORK.DESCRIPTION}>
  <TopLayout>
    <div class="animate page-heading">
      

<nav class="bg-white border-gray-200 dark:bg-gray-900">
  <div class="max-w-screen-xl flex flex-wrap items-center justify-center gap-6 mx-auto p-4">
    <a href="https://www.telefonica.com/es/" target="_blank" class="flex items-center space-x-3 rtl:space-x-reverse">
        <img src="logo-tele.jpg" class="h-8" alt="Logo" />
    </a>
    
    <div class="w-full md:block md:w-auto" id="navbar-default">
      <ul class="font-medium flex p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 rtl:space-x-reverse md:mt-0 md:border-0 md:bg-white dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700">
        <li>
          <a href="/work" class="block py-2 px-3 text-white bg-blue-700 rounded md:bg-transparent md:text-blue-700 md:p-0 dark:text-white md:dark:text-blue-500" aria-current="page">Auditorio</a>
        </li>
        <li>
          <a href="/consejo" class="block py-2 px-3 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent">Sala Consejo</a>
        </li>
      </ul>
    </div>
  </div>
</nav>

    </div>
  </TopLayout>
  <BottomLayout>
    <div class="flex gap-4 justify-center">

      <div class="flex flex-col justify-between max-w-xs bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
        <a href="/estructura5x5">
            <img class="w-64 h-32 mx-auto mt-4" src="5x5estruct.webp" alt="" />
        </a>
        <div class="p-5">
            <a href="/estructura5x5">
                <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white">Estructura Panatalla Led Sala Consejo</h5>
            </a>
        </div>
        <div class="p-5">
            <p class="mb-3 font-normal text-gray-700 dark:text-gray-400">Descripción de la estructura donde está soportada la pantalla LED</p>
        </div>
        <div class="p-5">
            <a href="/estructura5x5" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                Documento
                <svg class="rtl:rotate-180 w-3.5 h-3.5 ms-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
                </svg>
            </a>
        </div>
      </div>

      <div class="flex flex-col justify-between max-w-xs bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
        <a href="/datos5x5">
            <img class="w-64 h-32 mx-auto mt-4" src="5x5-datos.webp" alt="" />
        </a>
        <div class="p-5">
            <a href="/datos5x5">
                <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white">Cableado estructurado</h5>
            </a>
        </div>
        <div class="p-5">
            <p class="mb-3 font-normal text-gray-700 dark:text-gray-400">Esquema de conexión del cableado estructurado de la pantalla LED</p>
        </div>
        <div class="p-5">
            <a href="/datos" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                Documento
                <svg class="rtl:rotate-180 w-3.5 h-3.5 ms-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
                </svg>
            </a>
        </div>
      </div>

      <div class="flex flex-col justify-between max-w-xs bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
        <a href="/res5x5">
            <img class="w-64 h-32 mx-auto mt-4" src="5x5res.webp" alt="" />
        </a>
        <div class="p-5">
            <a href="/res5x5">
                <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white">Resolución Pantalla Led Auditorio</h5>
            </a>
        </div>
        <div class="p-5">
            <p class="mb-3 font-normal text-gray-700 dark:text-gray-400">Resolución del cabinet usado como de la pantalla LED</p>
        </div>
        <div class="p-5">
            <a href="/res5x5" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                Documento
                <svg class="rtl:rotate-180 w-3.5 h-3.5 ms-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
                </svg>
            </a>
        </div>
      </div>

      <div class="flex flex-col justify-between max-w-xs bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
        <a href="/elec5x5" class="p-2">
            <img class="w-64 h-32 mx-auto mt-4" src="5x5elec.webp" alt=""/>
        </a>
        <div class="p-5">
            <a href="/elec5x5">
                <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white">Cableado Eléctrico</h5>
            </a>
          </div>
          <div class="p-5">
            <p class="mb-3 font-normal text-gray-700 dark:text-gray-400">Esquema de conexión del cableado eléctrico de la pantalla LED</p>
          </div>
          <div class="p-5">
            <a href="/elec5x5" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                Documento
                <svg class="rtl:rotate-180 w-3.5 h-3.5 ms-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
                </svg>
            </a>
        </div>
      </div>
    </div>
  </BottomLayout>
</PageLayout>
